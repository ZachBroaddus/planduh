
<style id="style-1-cropbar-clipper">
.en-markup-crop-options {
    top: 18px !important;
    left: 50% !important;
    margin-left: -100px !important;
    width: 200px !important;
    border: 2px rgba(255,255,255,.38) solid !important;
    border-radius: 4px !important;
}

.en-markup-crop-options div div:first-of-type {
    margin-left: 0px !important;
}
</style>



<div class="container" >
<h2 class = "form-signin-heading">Discover Events</h2>

<% if @errors %>
<ul>
  <% @errors.each do |error| %>
    <li><%= error %></li>
  <% end %>
</ul>
<% end %>

<form class="form-signin" action="/sessions" method="POST">
<p>
  <input type="text" name="date" placeholder="date" class="form-control">
</p>
<p>
  <input type="text" name="begin_time" placeholder="time start" class="form-control">
</p>

<p>
  <input type="text" name="end_time" placeholder="time end" class="form-control">
</p>

<p>
  <input type="text" name="location" placeholder="location" class="form-control">
</p>


<p>
  <input type="submit" value="Find Event" placeholder="login" class="btn btn-lg btn-primary btn-block">
</p>
</form>
</div>
